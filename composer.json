{
    "name": "andrewgatenby/snapmigrations",
    "description": "Generates an SQL dump of your migrated (and seeded) database to save time re-running Migrations when running Integration and Acceptance Tests in a Lumen environment.",
    "keywords": [
        "lumen",
        "laravel",
        "phpunit",
        "run faster tests",
        "unit tests",
        "integration tests",
        "acceptance tests",
        "speed up tests",
        "fix slow tests",
        "make tests faster"
    ],
    "type": "project",
    "require": {
        "php": "^7.2",
        "dg/mysql-dump": "^1.5",
        "laravel/lumen-framework": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "AndrewGatenby\\SnapMigrations\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Andy Gatenby",
            "email": "andrew.gatenby@gmail.com"
        }
    ]
}
