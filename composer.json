    "type": "project",
    "require": {
        "php": "^7.2",
        "dg/mysql-dump": "^1.5",
        "laravel/lumen-framework": "5.7.*"
    },
    "autoload": {
        "psr-4": {
            "AndrewGatenby\\SnapMigrations\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
